<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.game103.ipahelper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FindActivity" />
        <activity android:name=".FindResultsActivity" />
        <activity android:name=".CompareActivity" />
        <activity android:name=".CompareResultsActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".ChartActivity"></activity>
    </application>

</manifest>